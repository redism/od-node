{
  "name": "od-node",
  "version": "1.1.0",
  "description": "Overdosed node library.",
  "main": "lib/index.js",
  "jsnext:main": "src/index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watchAll",
    "build": "babel src -d lib",
    "build:watch": "babel src -d lib -w",
    "lint": "eslint 'src/**/*.js' 'test/**/*.js'",
    "lint:fix": "eslint 'src/**/*.js' 'test/**/*.js' --fix",
    "lint:eslint": "eslint --ignore-path .gitignore --ignore-pattern lib",
    "publish": "npm run lint && npm run test && npm run build",
    "publish:force": "npm run build"
  },
  "author": "jeff.oh@odcode.com",
  "license": "MIT",
  "dependencies": {
    "bluebird": "3.5.3",
    "body-parser": "1.18.3",
    "btoa": "1.2.1",
    "cookie-parser": "1.4.4",
    "debug": "4.1.1",
    "express": "4.16.4",
    "jsonwebtoken": "8.4.0",
    "knex": "0.16.3",
    "lodash": "4.17.11",
    "mkdirp": "0.5.1",
    "moment": "2.24.0",
    "morgan": "1.9.1",
    "node-uuid": "1.4.8",
    "nodemailer": "5.1.1",
    "request": "2.88.0",
    "request-promise-native": "1.0.7",
    "rimraf": "2.6.3",
    "uuid": "3.3.2"
  },
  "devDependencies": {
    "@babel/cli": "7.2.3",
    "@babel/core": "7.3.3",
    "@babel/node": "7.2.2",
    "@babel/plugin-proposal-class-properties": "7.3.3",
    "@babel/plugin-proposal-decorators": "7.3.0",
    "@babel/plugin-proposal-do-expressions": "7.2.0",
    "@babel/plugin-proposal-export-namespace-from": "7.2.0",
    "@babel/plugin-proposal-json-strings": "7.2.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "7.2.0",
    "@babel/plugin-proposal-numeric-separator": "7.2.0",
    "@babel/plugin-proposal-optional-chaining": "7.2.0",
    "@babel/plugin-proposal-private-methods": "7.3.2",
    "@babel/plugin-proposal-throw-expressions": "7.2.0",
    "@babel/preset-env": "7.3.1",
    "@babel/register": "7.0.0",
    "babel-eslint": "10.0.1",
    "babel-plugin-transform-promise-to-bluebird": "2.0.0",
    "aws-sdk": "2.406.0",
    "bcrypt": "3.0.4",
    "eslint": "5.14.1",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-prettier": "4.0.0",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-jest": "22.3.0",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-react": "7.12.4",
    "husky": "1.3.1",
    "jest": "24.1.0",
    "lint-staged": "8.1.4",
    "mysql": "2.16.0",
    "od-js": "0.2.0",
    "prettier": "1.16.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,css,md}": [
      "eslint --fix",
      "prettier --write",
      "git add"
